include ../../../mixin
+matTitle('{{ "team.list"|translate }}','close')
form([formGroup]="data.form")
  div
    mat-card([stretch]="true")
      ag-grid-angular.ag-theme-material(
      #agGrid,
      style="height:330px;width:500px",
      [columnDefs]="teamGridService.columnDefs",
      [gridOptions]="teamGridService.gridOptions",
      [frameworkComponents]="teamGridService.frameworkComponents",
      (gridReady)="teamGridService.onGridReady($event)",
      [enableRtl]="true",
      rowHeight="35",
      headerHeight="35")
mat-dialog-actions
  button(mat-raised-button, color="primary", fxFlex, type="button", mat-dialog-close) {{ "close"|translate }}
